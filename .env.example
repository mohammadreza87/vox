# Firebase Configuration (public keys - safe to commit)
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# Server security
JWT_SECRET=replace_with_32_char_secret

# ElevenLabs - Voice synthesis and cloning (Server-side only)
ELEVENLABS_API_KEY=your_elevenlabs_api_key
# ElevenLabs Conversational AI Agent ID (create in ElevenLabs dashboard)
ELEVENLABS_AGENT_ID=your_agent_id

# Google Cloud / Vertex AI (Server-side only)
GOOGLE_CLOUD_PROJECT=your_project_name
GOOGLE_CLOUD_LOCATION=us-central1

# Claude - Anthropic AI (Server-side only)
ANTHROPIC_API_KEY=your_anthropic_api_key

# OpenAI - GPT models (Server-side only)
OPENAI_API_KEY=your_openai_api_key

# DeepSeek - Open source AI (Server-side only)
DEEPSEEK_API_KEY=your_deepseek_api_key

# Stripe Payments
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_publishable_key
NEXT_PUBLIC_STRIPE_PRICE_PRO_MONTHLY=
NEXT_PUBLIC_STRIPE_PRICE_PRO_ANNUAL=
NEXT_PUBLIC_STRIPE_PRICE_MAX_MONTHLY=
NEXT_PUBLIC_STRIPE_PRICE_MAX_ANNUAL=

# Redis (Rate limiting / caching)
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Monitoring
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=

# API URL (for production, this will be your Cloud Run URL)
NEXT_PUBLIC_API_URL=http://localhost:3000/api
